{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "target": "esnext",
    "jsx": "preserve",
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "."
  },
  "include": [
    "./src",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "pages/muscles/[sexType]"  ],
  "exclude": [
    "node_modules"
  ]
}
